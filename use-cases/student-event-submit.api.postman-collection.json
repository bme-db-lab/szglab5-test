				{
					"name": "Given working token",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"tests[\"Status code is 200\"] = responseCode.code === 200;",
									"var jsonData = JSON.parse(responseBody);",
									"postman.setEnvironmentVariable(\"token\", jsonData.token);",
									""
								]
							}
						}
					],
					"request": {
						"url": "{{url}}/auth/login",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n\t\"loginName\": \"joskapista\",\n\t\"password\": \"12345\"\n}"
						},
						"description": "{{url}}/auth/login"
					},
					"response": []
				},
				{
					"name": "Then I send a GET to /deliverables/1 and it should return a JSON which contains deadline attribute",
					"event": [
						{
							"listen": "test",
							"script": {
								"type": "text/javascript",
								"exec": [
									"tests[\"Status code is 200\"] = responseCode.code === 200;",
									"tests[\"Returns a deliverable with deadline\"] = responseBody.has(\"deadline\");"
								]
							}
						}
					],
					"request": {
						"url": "{{url}}/deliverables/1",
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}",
								"description": ""
							}
						],
						"body": {},
						"description": ""
					},
					"response": []
				}
			]
		}
